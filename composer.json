{
    "name": "fabiang/zend-pwnedpasswords",
    "description": "Validator for the Zend Framework for pwnedpasswords.com API",
    "type": "library",
    "keywords": [
        "zf2",
        "module",
        "zf2-module",
        "zf3-module",
        "zend-framework",
        "pwnedpasswords"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "f.grutschus@lubyte.de"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Fabiang\\ZendPwnedpasswords\\": "src/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "psr/log": "^1.0",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-servicemanager": "^2.7.8 || ^3.0",
        "zendframework/zend-validator": "^2.10"
    },
    "require-dev": {
        "zendframework/zend-modulemanager": "^2.2 || ^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "component": "Fabiang\\ZendPwnedpasswords",
            "config-provider": "Fabiang\\ZendPwnedpasswords\\ConfigProvider"
        }
    }
}
